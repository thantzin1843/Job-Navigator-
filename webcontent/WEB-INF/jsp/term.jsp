<script>
    function GenericPopupAlert(type, msg) // type 1 = OK, type 2= No Thanks
    {
        if(type == 1) // ok
        {
            $('.alert-container .alert-body .message').html(msg);
            $('.alert-body .img-container img').attr('src', '/employer/img/info-purple.png');
            $('.alert-body .img-container img').css('width', '90px');
            $('.alert-body .img-container img').css('height', '90px');
            $('.alert-body .img-container img').attr('width', '90');
            $('.alert-body .img-container img').attr('height', '90');
            $('.alert-container .btn-container a').removeClass();
            $('.alert-container .btn-container a').addClass('btn-generic-ok-to-continue');
            
            $('.alert-container').show();
            $('.btn-ok-to-continue').show();
            $('.btn-yes-no-thanks').removeClass('btn-generic-show');

        }
        else if(type == 2) // yes, no thanks
        {
            $('.alert-container .alert-body .message').html(msg);
            $('.alert-body .img-container img').attr('src', '/employer/img/question-purple.png');
            $('.alert-body .img-container img').css('width', '90px');
            $('.alert-body .img-container img').css('height', '90px');
            $('.alert-body .img-container img').attr('width', '90');
            $('.alert-body .img-container img').attr('height', '90');
            $('.alert-container .btn-yes-no-thanks .btn-yes-click').addClass('btn-generic-yes');
            $('.alert-container .btn-yes-no-thanks .btn-no-click').addClass('btn-generic-no-thanks');

            $('.alert-container').show();
            $('.btn-ok-to-continue').hide();
            $('.btn-yes-no-thanks').addClass('btn-generic-show');           
        }
    }
    
    $(document).on('click', '.alert-container .btn-container a', function (event) {
        $('.alert-container').hide();
    });

    $(document).on('click', '.alert-container .btn-yes-no-thanks a', function (event) {
        $('.alert-container').hide();
    });

    function GenericErrorPopupAlert(type, msg, btnmsg) // type 1 = OK, type 2= No Thanks
    {
        if (type == 1) // ok
        {
            $('.alert-container-error .alert-body-error .message').html(msg);
            $('.alert-body-error .img-container img').attr('src', '/employer/img/info-purple.png');
            $('.alert-body-error .img-container img').css('width', '90px');
            $('.alert-body-error .img-container img').css('height', '90px');
            $('.alert-body-error .img-container img').attr('width', '90');
            $('.alert-body-error .img-container img').attr('height', '90');
            $('.alert-container-error .btn-container a').removeClass();
            $('.alert-container-error .btn-container a').addClass('btn-generic-ok-to-continue-error');
            $('.alert-container-error .btn-container a').html(btnmsg);


            $('.alert-container-error').show();
            $('.btn-ok-to-continue-error').show();
        }

    }

    $(document).on('click', '.alert-container-error .btn-container a', function (event) {
        $('.alert-container-error').hide();
    });

</script>




